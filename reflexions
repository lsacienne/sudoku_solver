4 5 0 7 8 9 1 2 3 ---> 4 + 5 + 0 + 7 + 8 + 9 + 1 + 2 + 3 = 45 - x
[number, x_coord, y_coord]

4 5 6 0 8 9 1 0 3 ---> 4 + 5 + 6 + 0 + 8 + 9 + 1 + 0 + 3 = 45 - x - y 
---> - ( 4 + 5 + 6 + 8 + 9 + 1 + 3 - 45) = x + y

5 6 7 9 0 1 3 4 8 ---> 5 + 6 + 7 + 9 + 0 + 1 + 3 + 4 + 8 = 45 - y
---> - ( 5 + 6 + 7 + 9 + 1 + 3 + 4 + 8 - 45 ) = y

9 = x + y
2 =     y

[[1 , 1], * [[x], = [[9],
 [0 , 1]]    [y]]    [2]]